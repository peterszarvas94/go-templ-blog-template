package templates

templ SearchPage() {
	@head() {
		<script src="/static/search.js" type="module"></script>
		<script src="https://cdn.jsdelivr.net/npm/minisearch@7.1.0/dist/umd/index.min.js"></script>
	}
	@searchBody()
}

templ searchBody() {
	<body>
		@header()
		<main class="page search-page">
			<h1>Search</h1>
			<ul id="search-result" class="articles"></ul>
		</main>
		@footer()
	</body>
}
